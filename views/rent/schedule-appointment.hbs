<div style="text-align: center; width: 100%;margin-bottom:10%">
    <img style="display: block; margin: 0 auto; max-width: 100px;margin-top:70px;"
        src="data:image/png;base64," />
    <h5><b>Customers schedule</b></h5>
    {{#if usersAppointmentCount }}
    <div class="tableContainer">
        <span class="borderElement addBorder">Full name</span>
        <span class="borderElement addBorder">Date from</span>
        <span class="borderElement addBorder">Date to</span>
        <span class="borderElement addBorder">Hour from </span>
        <span class="borderElement addBorder">Hour to</span>
        <span class="borderElement addBorder lastItemBorder">Status</span>
    </div>
    {{/if}}
    {{#unless usersAppointmentCount }}
    <span>There is no scheduled users yet.</span>
    {{/unless}}

    {{#if isTheowner}}
    {{#each allScheduledUsersForRentForTable}}

    <div class="tableUser">
        <div class="borderElement tableColumnsProperties"><span>{{ fullName }}</span></div>
        <div class="borderElement tableColumnsProperties"><span>{{ dateStart }}</span>
          <span>({{dayEnd}}.</span>
            <span>{{monthEnd}}.</span>
            <span>{{yearEnd}})</span>
            </div>
        <div class="borderElement tableColumnsProperties"><span>{{ dateEnd }}</span>
          <span>({{dayEnd}}.</span>
            <span>{{monthEnd}}.</span>
            <span>{{yearEnd}})</span></div>
        <div class="borderElement tableColumnsProperties"><span>{{ startRentTime }}</span></div>
        <div class="borderElement tableColumnsProperties"><span>{{ endRentTime }}</span></div>

        {{#if isApproved}}
        <div id="container-button" class="borderElement lastItemBorder approved__style">
            <div name="statusResult" id="approved" class="leftSide"><span>{{ statusResult }}</span></div>
        </div>

        {{#if statusIsNotPending}}
        <div class="container__btn__makeAppointment">
            <form method="PUT" action="/makeAppointment/approved-rent/{{_id}}" class="container__form">
                <button type="submit">
                    <span class="checkmark">
                        <div class="checkmark_stem"></div>
                        <div class="checkmark_kick"></div>
                    </span>
                </button>
            </form>

            {{!-- <button style="font-size:26px;padding-top:10px;width:30px;height:30;cursor:pointer; "> --}}
                {{!-- </button> --}}
            <form method="PUT" action="/makeAppointment/declined-rent/{{_id}}" style="height: 100%;width:50%">
                <button type="submit" class="close btn__close" aria-label="Close" id="close">
                    {{!-- <div style="height: 100%;" aria-hidden="true">&times;</div> --}}
                    <div style="height: 100%;">
                        <p class="buttonX">x</p>
                    </div>
                </button>
            </form>

        </div>
        {{/if}}
        {{#unless statusIsNotPending}}
        {{/unless}}
        {{/if}}

        {{#unless isApproved}}
        <div id="container-button" class="borderElement"
            style="width:100%;height:100%;text-align:left; background-color:red;border-right: 3px solid green;">
            <div name="statusResult" id="close" class="leftSide"><span>{{ statusResult }}</span></div>
        </div>
        <div id="container-button" style="display: inline-flex;width: 100%;justify-content: center;">
            <form method="PUT" action="/makeAppointment/approved-rent/{{_id}}" style="height: 100%;width:50%">
                <button type="submit">
                    <span class="checkmark">
                        <div class="checkmark_stem"></div>
                        <div class="checkmark_kick"></div>
                    </span>
                </button>
            </form>

            <form method="PUT" action="/makeAppointment/declined-rent/{{_id}}" style="height: 100%;width:50%">
                <button type="submit" class="close btn__close" aria-label="Close" id="close">
                    <div style="height: 100%;">
                        <p class="buttonX">x</p>
                    </div>
                </button>

            </form>

        </div>
        {{/unless }}
    </div>
    {{/each}}
    {{/if}}

    {{#unless isTheowner}}
    {{#each allScheduledUsersForRentForTable}}
    <div class="tableUser">
        {{#if isApproved}}
        <div class="borderElement tableColumnsProperties"><span>{{ fullName }}</span></div>
        <div class="borderElement tableColumnsProperties">{{ dateStart }}</div>
        <div class="borderElement tableColumnsProperties">{{ dateEnd }}</div>
        <div class="borderElement tableColumnsProperties">{{ startRentTime }}</div>
        <div class="borderElement tableColumnsProperties">{{ endRentTime }}</div>
        <div id="container-button" class="approved__status">
            <span name="statusResult" id="approved" class="leftSide ">{{ statusResult }}</span>
        </div>
        {{/if }}
        {{#unless isApproved}}
        <div class="borderElement tableColumnsProperties"><span>{{ fullName }}</span></div>
        <div class="borderElement tableColumnsProperties" >
            <span style="margin-right:2px">{{dateStart}}</span>
            <span>({{dayStart}}.</span>
            <span>{{monthStart}}.</span>
            <span>{{yearStart}})</span>
        </div>
        <div class="borderElement tableColumnsProperties">
            <span>{{ dateEnd }}</span>
            <span>({{dayEnd}}.</span>
            <span>{{monthEnd}}.</span>
            <span>{{yearEnd}})</span>
        </div>
        <div class="borderElement tableColumnsProperties">{{ startRentTime }}</div>
        <div class="borderElement tableColumnsProperties">{{ endRentTime }}</div>
        <div id="container-button" class="decline-status borderElement tableColumnsProperties">
            <span name="statusResult" id="approved">{{ statusResult }}</span>
        </div>
        {{/unless }}
    </div>
    {{/each}}
    {{/unless}}

</div>